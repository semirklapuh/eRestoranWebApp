@model WebApplication1.Areas.ModulAdmin.ViewModels.Jelo.JeloVrstaDodaj

@{
    ViewData["Title"] = "Dodaj vrstu jela";


}


<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Dodaj vrstu jela</h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                    <li>
                        <a class="close-link"><i class="fa fa-close"></i></a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <br />
                <form asp-controller="Jelo" asp-action="DodajVrstuSave" method="post" id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">

                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" asp-for="Naziv">

                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <input asp-for="Naziv" type="text" class="form-control" name="Naziv">
                            <span asp-validation-for="Naziv"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-9 text-right">
                            <button type="reset" class="btn btn-primary">Resetiraj</button>
                            <button type="submit" class="btn btn-success">Potvrdi</button>

                        </div>
                    </div>
                </form>
            </div>

        </div>
    </div>
</div>


